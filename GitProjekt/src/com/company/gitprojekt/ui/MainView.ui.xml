<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE xml>

<?xdevuiml version="1.0"?>
<?template NONE?>
<?import com.vaadin.ui.CustomComponent?>
<?import com.xdev.ui.XdevButton?>
<?import com.xdev.ui.XdevVerticalLayout?>
<?import com.xdev.ui.XdevView?>
<?import com.xdev.ui.entitycomponent.table.XdevTable?>
<XdevView xmlns:x="http://xml.xdev-software.com/xdevuiml" x:name="MainView">
    <x:constraints zpos="0" width="100%" height="100%" />
    <XdevVerticalLayout x:name="verticalLayout">
        <x:constraints zpos="0" width="100%" height="100%" />
        <XdevButton caption="Button" x:name="button">
            <x:constraints weighty="0.0" />
        </XdevButton>
        <XdevButton caption="Button" x:name="button2">
            <x:constraints weighty="0.0" />
        </XdevButton>
        <XdevTable x:name="table">
            <x:constraints weighty="1.0" width="100%" height="100%" />
            <x:typeArguments>
                <typeArgument type="CustomComponent" />
            </x:typeArguments>
        </XdevTable>
    </XdevVerticalLayout>
</XdevView>

